@model List<Home>

<section id="home-section" class="hero">
	<div class="home-slider owl-carousel">
		@foreach (var item in Model)
		{
			
		
		<div class="slider-item">
		
			<div class="container-fluid px-md-0">
				<div class="row d-md-flex no-gutters slider-text align-items-end justify-content-end" data-scrollax-parent="true">
					<div class="one-third order-md-last img" style="@item.SocialMedia1">
					
						
					</div>
					<div class="one-forth d-flex  align-items-center ftco-animate" data-scrollax=" properties: { translateY: '70%' }">
						<div class="text">
							<span class="subheading">@item.Title</span>
							<h1 class="mb-4 mt-3"> @item.Description</h1>
							<p><a href="#" class="btn btn-primary">Bana Ulaş</a> <a href="#" class="btn btn-primary btn-outline-primary">Download CV</a></p>
						</div>
					</div>
				</div>
			</div>
		</div>
		}


	</div>
</section>@* HomePage*@